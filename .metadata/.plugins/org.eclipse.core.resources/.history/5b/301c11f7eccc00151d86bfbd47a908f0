package ass2;


 class Person {
	 
	 String name;
		double height;
		int weight;
		double bmi;
		
		
		public Person(String name, double height,double weight){
			
		
		}
	
		
public String indentity( String underweight,String normalWeight,String slightlyOverweight,String significantlyOverweight,String obesity){
		
		
	double bmi = height * height / weight;

		if (bmi < 20){
			
		
		return  underweight;
		}
		
		else if(bmi<=20 & bmi<25){
			
			return normalWeight;
		}
		
		else if(bmi<=25 & bmi < 30){
			
			return  slightlyOverweight;
		}
		else if(bmi<=30 & bmi < 40){
			
			return significantlyOverweight;
		}
		
		else if(bmi>=40){
			
			return  obesity;
		}
		else {
			
			return null;
		}
		
		
	}
	
	public String getInfo(){
		
		return name + " " + height + " " + weight+ " " + "=" + bmi;
	}
	
}
